{
  "name": "orchestermusik",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "npm run lilypond && docusaurus start",
    "build": "npm run lilypond && docusaurus build",
    "lilypond": "node scripts/compile-lilypond.js",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "setup": "npm i &&allow-scripts run"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.0",
    "@docusaurus/faster": "^3.9.0",
    "@docusaurus/preset-classic": "^3.9.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.9.0",
    "@docusaurus/tsconfig": "^3.9.0",
    "@docusaurus/types": "^3.9.0",
    "@lavamoat/allow-scripts": "^3.4.1",
    "@lavamoat/preinstall-always-fail": "^2.1.1",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  },
  "lavamoat": {
    "allowScripts": {
      "@docusaurus/core>@docusaurus/babel>@babel/runtime-corejs3>core-js-pure": false,
      "@docusaurus/core>core-js": false,
      "@docusaurus/faster>@swc/core": false,
      "@lavamoat/preinstall-always-fail": false
    }
  }
}
